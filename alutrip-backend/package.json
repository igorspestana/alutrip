{
  "name": "alutrip-backend",
  "version": "1.0.0",
  "description": "AluTrip Travel Assistant API Backend",
  "main": "dist/src/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate:dev": "tsx migrations/migrate.ts",
    "migrate": "node dist/migrations/migrate.js",
    "seed": "node dist/seeds/seed.js",
    "dc:up": "docker compose -f docker/docker-compose.yml --env-file ./.env up -d",
    "dc:down": "docker compose -f docker/docker-compose.yml --env-file ./.env down",
    "dc:ps": "docker compose -f docker/docker-compose.yml --env-file ./.env ps",
    "dc:logs": "docker compose -f docker/docker-compose.yml --env-file ./.env logs --tail=100 -f"
  },
  "dependencies": {
    "@google/generative-ai": "^0.3.0",
    "axios": "^1.6.0",
    "bullmq": "^5.58.5",
    "compression": "^1.7.0",
    "cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "groq-sdk": "^0.3.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "pg": "^8.11.0",
    "puppeteer": "^21.0.0",
    "redis": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^2.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.4.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "travel",
    "ai",
    "assistant",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "AluTrip Team",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
